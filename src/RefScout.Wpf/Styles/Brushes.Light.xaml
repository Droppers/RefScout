    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">

    <SolidColorBrush x:Key="Brushes.Background">#fff</SolidColorBrush>
    <SolidColorBrush x:Key="Brushes.BackgroundHover">#edebe9</SolidColorBrush>
    <SolidColorBrush x:Key="Brushes.BackgroundFocus">#d2d0ce</SolidColorBrush>
    <SolidColorBrush x:Key="Brushes.BackgroundDisabled">#ccc</SolidColorBrush>

    <SolidColorBrush x:Key="Brushes.Foreground">#000</SolidColorBrush>
    <SolidColorBrush x:Key="Brushes.ForegroundSecondary">#333</SolidColorBrush>
    <SolidColorBrush x:Key="Brushes.ForegroundDisabled">#555</SolidColorBrush>

    <SolidColorBrush x:Key="Brushes.ControlBackground">#fff</SolidColorBrush>
    <SolidColorBrush x:Key="Brushes.ControlDisabled">#333</SolidColorBrush>
    <SolidColorBrush x:Key="Brushes.ControlBackgroundHover">#e0e0e0</SolidColorBrush>
    <SolidColorBrush x:Key="Brushes.ControlBackgroundFocus">#ccc</SolidColorBrush>

    <SolidColorBrush x:Key="Brushes.Border">#ccc</SolidColorBrush>
    <SolidColorBrush x:Key="Brushes.BorderDark">#B2B2B2</SolidColorBrush>
    <SolidColorBrush x:Key="Brushes.BorderDisabled">#444</SolidColorBrush>

    <SolidColorBrush x:Key="Brushes.ScrollThumb">#d2d0ce</SolidColorBrush>
    <SolidColorBrush x:Key="Brushes.ScrollThumbHover">#bebbb8</SolidColorBrush>
    <SolidColorBrush x:Key="Brushes.ScrollThumbFocus">#a19f9d</SolidColorBrush>

    <SolidColorBrush x:Key="Brushes.Info">#00A2E8</SolidColorBrush>
    <SolidColorBrush x:Key="Brushes.Success">#5FAD61</SolidColorBrush>
    <SolidColorBrush x:Key="Brushes.Warning">#D89000</SolidColorBrush>
    <SolidColorBrush x:Key="Brushes.Error">#E05353</SolidColorBrush>

    <SolidColorBrush x:Key="Brushes.LanguageCSharp">#54AD54</SolidColorBrush>
    <SolidColorBrush x:Key="Brushes.LanguageVbNet">#185C92</SolidColorBrush>
    <SolidColorBrush x:Key="Brushes.LanguageFSharp">#9568CF</SolidColorBrush>
    <SolidColorBrush x:Key="Brushes.LanguageCppCli">#6295CB</SolidColorBrush>

    <SolidColorBrush x:Key="Brushes.DotNet">#6A4097</SolidColorBrush>
    <SolidColorBrush x:Key="Brushes.Gac">#D872E5</SolidColorBrush>

